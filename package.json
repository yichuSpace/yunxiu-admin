{
  "name": "yunxiu-admin-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "commit": "git-cz",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^17.0.25",
    "@typescript-eslint/parser": "^5.20.0",
    "@vueuse/core": "^8.3.1",
    "axios": "^0.26.1",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "pinia": "^2.0.13",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "sass": "^1.50.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run eslint",
      "npm run prettier"
    ]
  }
}